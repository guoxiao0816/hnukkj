<script>
    /*滑动加载*/
    var loading = false;
    $(document.body).infinite(100).on("infinite", function () {
        if (loading) return;
        loading = true;
        setTimeout(function () {
            for (var i = 0; i < 3; i++) {
                $('.commit_list').append(htmlobj.responseText);
                loading = false;
            }
