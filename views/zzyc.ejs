<script>
    var i;
    $(".screening div").click(function(){
        i=$(this).index();
        ifclass();
    })
    //    筛选下拉菜单选项选择效果
    $(".shaixuan span").click(function(){
        $(this).addClass("aui-btn-info").siblings("span").removeClass("aui-btn-info");
    })
    //    筛选下拉菜单提交按钮点击
    $(".shaixuan-submit").click(function(){
        ifclass();
    })

    //    筛选下拉菜单重置按钮点击
    $(".shaixuan-resize").click(function(){
        $(".shaixuan span").removeClass("aui-btn-info");
    })

    $(".choose-left li").click(function(){
        var j=$(this).index();
        $(this).addClass("now-list").siblings().removeClass("now-list");
        $(".choose-right").eq(j).addClass("choose-right-show").siblings(".choose-right").removeClass("choose-right-show");
    })
    $(".choose-text li").click(function(){
        var t=$(this).text();
        $(this).addClass("choose-now").siblings("li").removeClass("choose-now");
        $(".screening div").eq(i).text(t);
        ifclass();
    })
    $(".shadow").click(function(){
        ifclass();
    })
    function ifclass(){
        if($(".choose").eq(i).hasClass("choose-show")){
            $(".choose").eq(i).removeClass("choose-show")
            $(".shadow").hide();
        }else{
            $(".choose").eq(i).addClass("choose-show").siblings(".choose").removeClass("choose-show");
            $(".shadow").show();
        }
    }
</script>
<script type="text/javascript">
    //html5 定位功能
    $(function(){
        var json_data = [[113.512858,34.813433],[113.672442,34.816865],[113.669783,34.812249]];    //驾校坐标
        //HTML5 Web 存储
        if(typeof(Storage)!=="undefined")
        {
            localStorage.quanshunPointLng=113.512858;
            localStorage.quanshunPointlat=34.813433;
        }
        else
        {
            document.getElementById("result").innerHTML="对不起，您的浏览器不支持 web 存储。";
        }

        //判断是否支持html定位功能
        if (navigator.geolocation)
        {
            navigator.geolocation.getCurrentPosition(translatePoint);
        }
        else
        {
            alert("该浏览器不支持获取地理位置。")
        }
        function translatePoint(position){
            var currentLat = position.coords.latitude;
            var currentLon = position.coords.longitude;
            var gpsPoint = new BMap.Point(currentLon, currentLat);
            BMap.Convertor.translate(gpsPoint, 0, initMap); //转换坐标
        }
        function initMap(point){
            //初始化地图
            var map = new BMap.Map("allmap");
            var pointB = new BMap.Point(point.lng,point.lat); //用户坐标
            for(var i=0;i<json_data.length;i++){      //遍历驾校坐标数组
                var pointA = new BMap.Point(json_data[i][0],json_data[i][1]);  // 为每个驾校坐标创建点坐标A
                $(".distance"+i).text((map.getDistance(pointA,pointB)).toFixed(0)+"米");//获取两点距离,保留小数点后两位
            }
        }
        // 百度地图API功能
    });
</script>
