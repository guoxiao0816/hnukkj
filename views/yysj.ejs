                        $.toast("<p style='color: #fff;'>约车成功</p>");
                    },
                    onCancel: function () {

                    }
                });
            }else {
                alert("每天最多可预约两次");
            }
        })
        $('.close_login').click(function (e) {
            $(this).parent().hide();
            e.stopPropagation();
        });
        $('.close_prompt').click(function (e) {
            $(this).parent().hide();
            e.stopPropagation();
        })
    })
