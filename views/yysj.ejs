<script>
    /*顶部滑动约车*/
    $(function () {
        var swiper_tab = new Swiper('#tab', {
            freeMode: true,
            freeModeMomentumRatio: 0.5,
            slidesPerView: 'auto'
        });
        swiperWidth = swiper_tab.container[0].clientWidth
        maxTranslate = swiper_tab.maxTranslate();
        maxWidth = -maxTranslate + swiperWidth / 2
        swiper_tab.on('tap', function (swiper, e) {
            e.preventDefault()
            slide = swiper.slides[swiper.clickedIndex]
            console.log(swiper.clickedIndex);
            slideLeft = slide.offsetLeft
            slideWidth = slide.clientWidth
            slideCenter = slideLeft + slideWidth / 2
            // 被点击slide的中心点
            swiper_tab.setWrapperTransition(300)
            //设置偏移量
            if (slideCenter < swiperWidth / 2) {
                swiper_tab.setWrapperTranslate(0)
            } else if (slideCenter > maxWidth) {
                swiper_tab.setWrapperTranslate(maxTranslate)
            } else {
                nowTlanslate = slideCenter - swiperWidth / 2
                swiper_tab.setWrapperTranslate(-nowTlanslate)
            }
            //添加样式
            $("#tabtime .active").removeClass('active')
            $("#tabtime .swiper-slide").eq(swiper.clickedIndex).addClass('active')
        })
    })
</script>
